<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="zfbzx">
	
	<!--获取社保信息列表-->
	<select id="queryInfo"  resultType="ZfbzxModel">
	SELECT PZR.ZFB_ID MODELID,
	PZR.APPLICATION_ID APPID,
	PZR.IDENTITY_CARD CRETID,
	'01' CRETTYPE,
	PZR.USER_NAME NAME,
	PZR.REGISTTIME REGISTTIME,
	PZR.TAOBAOTRADE TAOBAOTRADE,
	PZR.BESTUDENT BESTUDENT,
	PZR.BIZCUSTOMER BIZCUSTOMER,
	PZR.VERIFYSCORE VERIFYSCORE,
	PZR.REALTYPE,
	PZR.TRADEINFOLIFE TRADEINFOLIFE,
	PZR.TRADEINFOFINANCE TRADEINFOFINANCE,
	PZR.TRADEINFOMATERIAL TRADEINFOMATERIAL,
	PZR.ADDRSCORE ADDRSCORE,
	PZR.BINDFASTPAYMENT BINDFASTPAYMENT,
	PZR.CUSTLEVEL CUSTLEVEL,
	PZR.CONSUMESINGLE CONSUMESINGLE,
	PZR.CONSUMEMONTHLY CONSUMEMONTHLY,
	PZR.CAPITALFUND CAPITALFUND,
	PZR.COSUMELUXURY COSUMELUXURY,
	PZR.ZMSCORE ZMSCORE,
	PZR.CREDITHISTORY CREDITHISTORY,
	PZR.HONESTYSCENE HONESTYSCENE,
	PZR.DEADBEAT DEADBEAT,
	PZR.CONSUMELEVEL CONSUMELEVEL,
	PZR.CONSUMESTABILITY CONSUMESTABILITY,
	PZR.PAYLEVEL PAYLEVEL,
	PZR.ALIPAYSCENE ALIPAYSCENE,
	PZR.INTEREST INTEREST,
	PZR.SNSINFLUENCE SNSINFLUENCE,
	PZR.CONTACTSSTABILITY CONTACTSSTABILITY,
	PZR.ADDRESSFIXEDIXEDDAYS ADDRESSFIXEDDAYS,
	PZR.MOBILEFIXEDIXEDDAYS MOBILEFIXEDDAYS
	FROM PF_ZFB_RECORD PZR
	WHERE PZR.FLAG = '0'
		
	</select>
	
	
		
	<!-- 更新上传ftp后社保数据的状态 -->
	<update id="updataFlag" parameterType="ZfbzxModel">
UPDATE PF_ZFB_RECORD A SET A.FLAG='1' WHERE ZFB_ID = #{modelid}
	</update>
</mapper>
