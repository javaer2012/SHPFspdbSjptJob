<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ApplicationMapper">

	<select id="queryApplicationInfoById" parameterType="ApplicationModel" resultType="ApplicationModel">
		select * from pf_application pa where pa.shenqingshucode = #{shenqingshucode}
	</select>
	
</mapper>
